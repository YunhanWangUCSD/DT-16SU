<?xml version="1.0"?>
<configuration>
    <!-- <property> <name>dt.application.{appName}.operator.{opName}.prop.{propName}</name>
      <value>some-default-value (if value is not specified, it is required from
      the user or custom config when launching)</value> </property> -->
    <!-- memory assigned to app master <property> <name>dt.attr.MASTER_MEMORY_MB</name>
      <value>1024</value> </property> -->
    <!-- URL to connect to the DB master -->
    <property>
        <name>dt.application.FileToJdbcApp.operator.JdbcOutput.prop.store.databaseDriver
        </name>
        <value>com.mysql.jdbc.Driver</value>
    </property>

    <property>
        <name>dt.application.FileToJdbcApp.operator.JdbcOutput.prop.store.databaseUrl
        </name>
        <value>jdbc:mysql://node17.morado.com:5505/testDev</value>
    </property>

    <property>
        <name>dt.application.FileToJdbcApp.operator.JdbcOutput.prop.store.userName
        </name>
        <value>root</value>
    </property>

    <property>
        <name>dt.application.FileToJdbcApp.operator.JdbcOutput.prop.store.password
        </name>
        <value>password</value>
    </property>

    <!-- # rows that the operator can retrieve in a window -->
    <property>
        <name>dt.application.FileToJdbcApp.operator.JdbcOutput.prop.batchSize
        </name>
        <value>5</value>
    </property>

    <!-- Table name -->
    <property>
        <name>dt.application.FileToJdbcApp.operator.JdbcOutput.prop.tablename
        </name>
        <value>test_jdbc_table</value>
    </property>

    <!-- POJO class -->
    <property>
        <name>dt.application.FileToJdbcApp.operator.JdbcOutput.port.input.attr.TUPLE_CLASS
        </name>
        <value>com.example.FileToJdbcApp.PojoEvent</value>
    </property>


    <property>
        <name>dt.application.FileToJdbcApp.operator.reader.prop.directory</name>
        <value>/user/yunhan</value>
    </property>

    <property>
        <name>dt.loggers.level</name>
        <value>com.datatorrent.*:INFO,org.apache.*:INFO</value>
    </property>

    <property>
        <name>dt.application.FileToJdbcApp.operator.reader.prop.directory</name>
        <value>/user/yunhan</value>
    </property>
    <!--<property>-->
    <!--<name>dt.application.FileToJdbcApp.operator.fileReader.prop.partitionCount</name>-->
    <!--<value>5</value>-->
    <!--</property>-->
    <property>
        <name>dt.loggers.level</name>
        <value>com.datatorrent.*:INFO,org.apache.*:INFO</value>
    </property>

</configuration>